class Solution:
    def numberOf2sInRange(self, n: int) -> int:
        cur = n % 10
        high = n // 10
        low = 0
        digit = 1
        res = 0
        while cur or high:
            if cur <2:
                res += high * digit
            elif cur == 2:
                res += high * digit + low + 1
            else:
                res += (high + 1) * digit
            low += cur * digit
            cur = high % 10
            high //= 10
            digit *= 10
        return res
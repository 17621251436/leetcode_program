##前缀和数组
class Solution:
    def subarraySum(self, nums: List[int], k: int) -> int:
          cnt,n=0,len(nums)
          dp=[0]*(n+1)
          for i in range(1,n+1):
              dp[i]+=dp[i-1]+nums[i-1]

          for i in range(1,n+1):
              for j in range(i,n+1):
                   if dp[j]-dp[i-1]==k:
                        cnt+=1
          return cnt








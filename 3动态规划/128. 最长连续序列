class Solution:
    def longestConsecutive(self, nums: List[int]) -> int:
        if not nums:
            return 0
        set_nums=set(nums)
        max_length=0
        cur_num=0
        cur_len=0
        for num in set_nums:
            if num-1 not in set_nums:
                cur_num=num
                cur_len=1

            while  cur_num+1 in set_nums:
                cur_num+=1
                cur_len+1
            max_length =max(cur_len,max_length)
        return max_length
